APP_NAME="Warehouse Management System"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=id
APP_FALLBACK_LOCALE=id
APP_FAKER_LOCALE=en_ID

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=warehouse_db
DB_USERNAME=root
DB_PASSWORD=

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_PATH=/backups
BACKUP_RETENTION_DAYS=30

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local

# Queue Configuration
# For production, use redis for better performance
QUEUE_CONNECTION=database
# QUEUE_CONNECTION=redis

# Cache Configuration
# For production, use redis for better performance
CACHE_STORE=database
# CACHE_STORE=redis
CACHE_PREFIX=wms_cache

MEMCACHED_HOST=127.0.0.1

# Redis Configuration
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail Configuration
MAIL_MAILER=smtp
MAIL_SCHEME=null
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS="noreply@warehouse.com"
MAIL_FROM_NAME="${APP_NAME}"

# AWS Configuration (for S3 storage if needed)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Sentry Configuration (Error Tracking)
SENTRY_LARAVEL_DSN=
SENTRY_TRACES_SAMPLE_RATE=1.0
SENTRY_PROFILES_SAMPLE_RATE=1.0

# Telescope Configuration (Development Monitoring)
TELESCOPE_ENABLED=false
TELESCOPE_PATH=telescope

# Rate Limiting Configuration
RATE_LIMIT_PER_MINUTE=60
API_RATE_LIMIT_PER_MINUTE=60

VITE_APP_NAME="${APP_NAME}"
